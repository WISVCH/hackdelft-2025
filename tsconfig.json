{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "noImplicitAny": true,
        "sourceMap": true,
        "declaration": false,
        "outDir": "./dist",
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "node",
        "jsx": "react",
        "jsxFactory": "h",
        "allowJs": false,
        "baseUrl": "./src",
        "paths": {
            "/*": ["../*"]
        },
        "resolveJsonModule": true,
        "esModuleInterop": true
    },
    "include": ["./src/**/*", "./static/**/*"],
    "exclude": ["node_modules"]
}
